{
  "name": "pbrute",
  "version": "0.1.0",
  "description": "A password strength calculator",
  "main": "lib/index.js",
  "scripts": {
    "lint": "xo",
    "test": "ava",
    "coverage": "nyc ava -t",
    "check-duplicate-code": "jsinspect -t 20 src",
    "build": "./build.sh"
  },
  "author": "Alex Gustafsson",
  "license": "MIT",
  "devDependencies": {
    "ava": "^2.4.0",
    "jsinspect": "^0.12.7",
    "nyc": "^14.1.1",
    "xo": "^0.25.3"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "xo": {
    "space": 2,
    "esnext": true,
    "rules": {
      "curly": [
        "error",
        "multi-or-nest",
        "consistent"
      ],
      "no-use-before-define": [
        "error",
        {
          "classes": false
        }
      ],
      "unicorn/catch-error-name": [
        "error",
        {
          "name": "error"
        }
      ],
      "dot-notation": [
        "off"
      ],
      "quote-props": [
        "off"
      ],
      "object-curly-spacing": [
        "off"
      ],
      "no-new": [
        "off"
      ],
      "padding-line-between-statements": [
        "off"
      ]
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "concurrency": 5,
    "failFast": false,
    "verbose": true,
    "compileEnhancements": false
  }
}
